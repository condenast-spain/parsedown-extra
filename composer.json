{
    "name": "condenast-spain/parsedown-extra",
    "description": "An extension of Parsedown that adds support for Markdown Extra.",
    "keywords": ["markdown", "markdown extra", "parser", "parsedown"],
    "homepage": "https://github.com/condenast-spain/parsedown-extra",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "CondeNast Spain",
            "homepage": "http://www.cnworld.es/"
        }
    ],
    "require": {
        "condenast-spain/parsedown": "^1.7.2",
        "php": ">=5.3.6",
        "ext-dom": "*",
        "ext-mbstring": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8.35"
    },
    "autoload": {
        "psr-0": {"ParsedownExtra": ""}
    },
    "autoload-dev": {
        "psr-0": {
            "TestParsedown": "test/",
            "ParsedownExtraTest": "test/",
            "ParsedownTest": "vendor/condenast-spain/parsedown/test/",
            "CommonMarkTest": "vendor/condenast-spain/parsedown/test/",
            "CommonMarkTestWeak": "vendor/condenast-spain/parsedown/test/"
        }
    }
}
